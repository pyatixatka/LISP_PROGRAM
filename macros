
;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=

;;; Задание №1
;;; Зинченко В.Д.
;;; Определить макрос, который возвращает свой вызов.
;;;

(defmacro Свой_Вызов (&rest x) `(quote (Свой_Вызов ,@x)))
  
;;;
;;; (Свой_Вызов a b c d e)
;;; - (Свой_Вызов A B C D E)
;;; (Свой_Вызов nil)
;;; - (Свой_Вызов NIL)
;;;

;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=;=
